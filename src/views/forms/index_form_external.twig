{% extends 'datatable.twig' %}
{% block js %}
    {{ parent() }}
    <script>
        let variables = {
            formReferenceExternal: '{{ formReferenceExternal }}',
        }
        let language = {
            translations: {{ translations|json_encode|raw }},
        }
    </script>
    <script src="{{ BASE_URL }}/vendor/tigress/form-builder/public/javascript/forms/index_form_external.js"></script>
{% endblock %}
{% block title %}{{ __('Forms') }} - {{ __('Overview') }}{% endblock %}
{% block content %}
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-12">
                <h3>{{ __('Overview') }} - {{ form.name }}</h3>
            </div>
        </div>
        <div class="row mb-2">
            <div class="col-md-12">
                <fieldset class="mt-2">
                    <div class="row mb-2">
                        <div class="col-sm-12 table-responsive">
                            <table id="dataTableAnswers" class="table table-striped table-bordered"></table>
                        </div>
                    </div>
                </fieldset>
            </div>
        </div>
    </div>
{% endblock %}