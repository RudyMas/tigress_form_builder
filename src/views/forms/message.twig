{% extends 'base.twig' %}
{% block content %}
<div class="container-fluid">
    <div class="row mb-2">
        <div class="col-sm-12">
            {{ message|raw }}
        </div>
    </div>
    {% if showButton %}
    <div class="row mb-2">
        <div class="col-sm-12">
            <a href="{{ urlForm }}" class="btn btn-primary">{{ __('New entry') }}</a>
        </div>
    </div>
    {% endif %}
</div>
{% endblock %}